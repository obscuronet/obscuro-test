<?xml version="1.0" encoding="utf-8"?>
<pysystest type="auto">

    <description>
        <title>Allocate OBX game funds to test users</title>
        <purpose><![CDATA[
        The pre-funded deployment account which has tokens allocated to it in the L2 will iterate through all test user
        accounts and fund them.
]]>
        </purpose>
    </description>

    <classification>
        <groups inherit="true">
            <group></group>
        </groups>
        <modes inherit="true">
            <mode>obscuro.dev</mode>
            <mode>obscuro.local</mode>
        </modes>
    </classification>

    <data>
        <class name="PySysTest" module="run"/>
    </data>

    <traceability>
        <requirements>
            <requirement id=""/>
        </requirements>
    </traceability>
</pysystest>
